<Window x:Class="ORCA.cadFunc_adm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ORCA"
        mc:Ignorable="d"
        Title="cadFunc_adm" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Margin="327,109,0,0" VerticalAlignment="Top" Width="200" />
        <TextBox x:Name="txtSenha" HorizontalAlignment="Left" Margin="327,152,0,0" VerticalAlignment="Top" Width="200" />

        <ComboBox x:Name="cmbPermissao" HorizontalAlignment="Left" Margin="327,201,0,0" VerticalAlignment="Top" Width="200"></ComboBox>
        
        <Button x:Name="btn_cad_func" Content="Cadastrar Funcionário" HorizontalAlignment="Left" Margin="327,267,0,0" VerticalAlignment="Top" Width="200" Click="btn_cad_func_Click"/>

    </Grid>
</Window>
